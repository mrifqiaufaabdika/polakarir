      "valid-v-else-if": "warn",
      "valid-v-else": "warn",
      "valid-v-for": "warn",
      "valid-v-html": "warn",
      "valid-v-if": "warn",
      "valid-v-model": "warn",
      "valid-v-on": "warn",
